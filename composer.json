{
    "name": "faustbrian/laravel-tax-calculator",
    "description": "Simple Tax Calculator for Laravel",
    "keywords": ["laravel", "framework", "Laravel-Tax-Calculator", "Laravel Tax Calculator", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.*",
        "illuminate/database": "5.3.* || 5.4.* || 5.5.*",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.3",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\TaxCalculator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\TaxCalculator\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\TaxCalculator\\TaxCalculatorServiceProvider"
            ],
            "aliases": {
                "TaxCalculator": "BrianFaust\\TaxCalculator\\Facades\\TaxCalculator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
